{
    "version": 2,
    "builds": [
        {
            "src": "dist/src/server.js",
            "use": "@vercel/node",
            "config": { "includeFiles": ["dist/**"] }
        }
    ],
    "routes": [
        { "handle": "filesystem" },
        { 
            "src": "/routes/(.*)", 
            "dest": "dist/src/routes/index.js" 
            }, 
        { 
            "src": "/(.*)", 
            "dest": "dist/src/server.js" 
            } 
      ]
}